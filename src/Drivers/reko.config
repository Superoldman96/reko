	  <Architecture Name="arm" Description="ARM 32-bit" Type="Reko.Arch.Arm.Arm32Architecture,Reko.Arch.Arm">
		<ProcedurePrologs>
		  <Pattern>
			  <Bytes>E9 2D 00 00  E2 4D D0 00</Bytes>   <!-- push {rn..rm}; sub sp,sp,imm -->
			  <Mask> FF FF A0 00  FF FF F0 00</Mask>
		  </Pattern>  
		  <Pattern>
			  <Bytes>E9 2D 00 00</Bytes>                <!-- push {rn..rm} -->
			  <Mask> FF FF A0 00</Mask>
		  </Pattern>  
		  <Pattern>
			  <Bytes>ED 2D 0A 00</Bytes>                <!-- push {vn..vm} -->
			  <Mask> FF BF 0E 00</Mask>
		  </Pattern> 
		  <Pattern>
			  <Bytes>E5 2D E0 04</Bytes>                <!-- push {lr} -->
			  <Mask> FF FF FF FF</Mask>
		  </Pattern>
